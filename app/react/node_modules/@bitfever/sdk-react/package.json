{
  "name": "@bitfever/sdk-react",
  "version": "1.0.6",
  "type": "module",
  "description": "BitFever SDK React component for integrating BitFever functionality into React applications.",
  "keywords": [
    "bitfever",
    "sdk",
    "react",
    "component",
    "iframe"
  ],
  "homepage": "https://bitfever.com",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitfever/bitfever.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsup --dts",
    "build:fast": "tsup",
    "build:app": "vite build",
    "preview": "vite preview",
    "clean": "rimraf dist dist-app",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "clean-package": "^2.2.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.5.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0"
  }
}
