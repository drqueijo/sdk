{
  "name": "@bitfever/sdk-vue",
  "version": "1.0.6",
  "type": "module",
  "description": "BitFever SDK Vue component for integrating BitFever functionality into Vue applications.",
  "keywords": [
    "bitfever",
    "sdk",
    "vue",
    "component",
    "iframe"
  ],
  "homepage": "https://bitfever.com",
  "main": "dist/index.umd.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitfever/bitfever.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --config vite.config.mts",
    "build:app": "vite build",
    "preview": "vite preview",
    "clean": "rimraf dist dist-app",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^22.16.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "clean-package": "^2.2.0",
    "eslint": "^9.29.0",
    "eslint-plugin-vue": "^9.0.0",
    "globals": "^16.2.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.5.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "unplugin-vue-define-options": "3.0.0-beta.15",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.0.0"
  }
}
